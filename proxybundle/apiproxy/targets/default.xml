<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <DefaultFaultRule name="default-fault">
    <Step>
      <Name>JS-faultMessage</Name>
    </Step>
  </DefaultFaultRule>
  <HTTPTargetConnection>
    <Authentication>
      <GoogleAccessToken>
        <Scopes>
          <Scope>https://www.googleapis.com/auth/cloud-platform</Scope>
        </Scopes>
      </GoogleAccessToken>
    </Authentication>
    <URL>https://us-central1-aiplatform.googleapis.com/v1/projects/apigee-instance10/locations/us-central1/publishers/google/models/{llm.model}:generateContent</URL>
    <Properties>
      <Property name="io.timeout.mills">120000</Property>
    </Properties>
  </HTTPTargetConnection>
</TargetEndpoint>
